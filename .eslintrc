{
  "parser": "babel-eslint",
  "plugins": [
      "react"
  ],
  "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "env": {
      "es6":     true,
      "browser": true,
      "node":    true,
      "jest":    true
  },   
  "extends": [
      "eslint:recommended",
      "google",
      "plugin:react/recommended"
  ],
  "rules": {
      "require-jsdoc": 0,
      "no-invalid-this": 0,
      "no-console": 0
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}